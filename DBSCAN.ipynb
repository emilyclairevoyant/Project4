   "source": [
    "### Run a grid search to find the best parameters for the DBSCAN\n",
    "*Note* Only use with small dataset"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "3b6e667d-eb34-4271-a02b-0ddc95ff131c",
   "metadata": {},
   "outputs": [],
   "source": [
    "epsilon = np.linspace(0.5, 1, num = 20)\n",
    "epsilon"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "f921c263-6602-44f7-886c-ec664efa0cbf",
   "metadata": {},
   "outputs": [],
   "source": [
    "min_samples = np.arange(2, 40, step=4)\n",
    "min_samples"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "99c45dcf-9ab4-4539-bc9a-a13a5d0e9f31",
   "metadata": {},
   "outputs": [],
   "source": [
    "import itertools\n",
    "combinations = list(itertools.product(epsilon, min_samples))\n",
    "N = len(combinations)\n",
    "N"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "263ac583-756b-4fe1-92bb-6601a5049f19",
   "metadata": {},
   "outputs": [],
   "source": [
    "#metrics = ['l1', 'l2', 'mahattan']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "e74f5910-7553-44ee-84c3-207c9d3e0936",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.metrics import silhouette_score as shs\n",
    "def Gridsearch(combinations, X):\n",
    "    scores = []\n",
    "    all_label = []\n",
    "\n",
    "    for i,(eps, num_samples) in enumerate(combinations):\n",
    "        dbscan_model = DBSCAN(eps = eps, min_samples = num_samples, metric = 'l2').fit(X)\n",
    "        labels = dbscan_model.labels_\n",
    "        labels_set = set(labels)\n",
    "        num_clusters = len(labels_set)\n",
    "        if -1 in labels_set:\n",
    "            num_clusters -= 1\n",
    "        if(num_clusters < 2) or (num_clusters > 100):\n",
    "            scores.append(-20)\n",
    "            all_label.append('Poor')\n",
    "            print(' at iteration: ',i,'ep = ',eps,'min_sample = ',num_samples,'number of cluster= ',num_clusters,'moving on..')\n",
    "            continue\n",
    "        scores.append(shs(X, labels))\n",
    "        all_label.append(labels)\n",
    "        print(' at iteration:',i,'score :',scores[-1],'Number of clusters :', num_clusters )\n",
    "    best_index = np.argmax(scores)\n",
    "    best_parameters = combinations[best_index]\n",
    "    best_labels = all_label[best_index]\n",
    "    best_score = scores[best_index]\n",
    "    return{'best_epsilon': best_parameters[0],\n",
    "           'best_min_samples' : best_parameters[1],\n",
    "            'best_labels' : best_labels,\n",
    "            'best_score' : best_score }"
   ]
  },

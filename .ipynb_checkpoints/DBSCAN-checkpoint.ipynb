   "source": [
    "### Run a grid search to find the best parameters for the DBSCAN\n",
    "*Note* Only use with small dataset"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 48,
   "id": "3b6e667d-eb34-4271-a02b-0ddc95ff131c",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "array([0.5       , 0.52631579, 0.55263158, 0.57894737, 0.60526316,\n",
       "       0.63157895, 0.65789474, 0.68421053, 0.71052632, 0.73684211,\n",
       "       0.76315789, 0.78947368, 0.81578947, 0.84210526, 0.86842105,\n",
       "       0.89473684, 0.92105263, 0.94736842, 0.97368421, 1.        ])"
      ]
     },
     "execution_count": 48,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "epsilon = np.linspace(0.5, 1, num = 20)\n",
    "epsilon"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 49,
   "id": "f921c263-6602-44f7-886c-ec664efa0cbf",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "array([ 2,  6, 10, 14, 18, 22, 26, 30, 34, 38])"
      ]
     },
     "execution_count": 49,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "min_samples = np.arange(2, 40, step=4)\n",
    "min_samples"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 50,
   "id": "99c45dcf-9ab4-4539-bc9a-a13a5d0e9f31",
   "metadata": {},
   "outputs": [
